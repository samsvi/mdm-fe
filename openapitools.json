{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "generator-cli": {
    "useDocker": true,
    "version": "6.6.0",
    "generators": {
      "ambulance-wl": {
        "generatorName": "typescript-fetch",
        "glob": "api/mdm.openapi.yaml",
        "output": "#{cwd}/src/api/mdm",
        "additionalProperties": {
          "supportsES6": "true",
          "withInterfaces": true
        }
      }
    }
  }
}
